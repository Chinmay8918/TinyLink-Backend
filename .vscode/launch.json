{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug Node.js App",
      "program": "${workspaceFolder}/index.js",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "PORT": "5000",
        "ACCESS_TOKEN_SECRET": "your_access_token_secret_here",
        "REFRESH_TOKEN_SECRET": "your_refresh_token_secret_here"
      }
    }
  ]
}
